@using ITVComponents.WebCoreToolkit.Net.Extensions
@using ITVComponents.WebCoreToolkit.Net.TelerikUi.Extensions
@using Kendo.Mvc.UI

@model ITVComponents.WebCoreToolkit.Net.TelerikUi.TenantSecurityViews.ViewModel.HealthScriptViewModel
@Html.HiddenFor(n => n.HealthScriptId)
<div class="form-group">
    <label for="HealthScriptName" class="dropdown-header">Name</label>
    @Html.Kendo().TextBoxFor(n => n.HealthScriptName)
</div>
<div class="form-group">
    <label for="Script" class="dropdown-header">Script</label>
    @Html.TextAreaFor(x => x.Script, new { @class = "k-textbox", style = "width: 100%; height:150px;" })
</div>